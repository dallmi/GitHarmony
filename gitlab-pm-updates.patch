+  return {
+    status,
+    reason: primaryReason,
+    severity,
+    factors,
+    actions,
+    metrics: {
+      progressPercent,
+      remainingIssues,
+      closedIssues: closedIssues.length,
+      totalIssues,
+      remainingIterations,
+      currentVelocity,
+      requiredVelocity,
+      velocityRatio,
+      blockedCount,
+      totalWeight,
+      weightVariance,
+      daysUntilDue,
+      overallocatedMembers
+    },
+    projection
+  }
